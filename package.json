{
  "name": "luck-node-mtp",
  "version": "1.0.0",
  "author": "lucksoft",
  "license": "ISC",
  "description": "cross-platform android device file management tool, based on libmtp. Support `MacOs`, `Windows`, `linux` platforms.",
  "homepage": "https://github.com/lucksoft-yungui/luck-node-mtp",
  "bugs": {
    "url": "https://github.com/lucksoft-yungui/luck-node-mtp/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "pretest": "cross-env NODE_ENV=development node test/test.js",
    "posttest": "cross-env NODE_ENV=production node test/test.js",
    "build:dev": "node-gyp configure && node-gyp rebuild --debug",
    "build": "node-gyp build",
    "clean": "node-gyp clean",
    "prebuild": "prebuildify --napi --strip && node install/after.js",
    "install": "node-gyp-build",
    "dist": "cross-env NODE_ENV=production tsup"
  },
  "gypfile": true,
  "keywords": [
    "mtp",
    "node",
    "android",
    "file management"
  ],
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^1.1.0",
    "node-gyp-build": "^4.6.0",
    "shelljs": "^0.8.5",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "cross-env": "^7.0.3",
    "node-gyp": "^9.4.0",
    "tsup": "^7.0.0"
  }
}
